{
    "default": { 
        "type": "default"
    },
    "icon-text-default": {
        "title": "Notification icon/text",
        "text": "Icon Text Default Content"
    },
    "dismissable": {
        "dismissable": true,
        "title": "Notification Dismissable",
        "text": "Dismissable Content"
    },
    "top-fix": {
        "position": "top-fix",
        "title": "Notification Top Fix",
        "text": "Top Fix Content",
        "timeout": 5000
    },
    "bottom-fix": {
        "position": "bottom-fix",
        "type": "error",
        "icon": "it-close-circle",
        "title": "Notification Bottom Fix",
        "text": "Bottom Fix Content",
        "timeout": 8000
    },
    "left-fix": {
        "position": "left-fix",
        "type": "info",
        "icon": "it-info-circle",
        "title": "Notification Left Fix",
        "text": "Left Fix Content",
        "timeout": 6000
    },
    "right-fix": {
        "position": "right-fix",
        "type": "warning",
        "icon": "it-error",
        "title": "Notification Right Fix",
        "text": "Right Fix Content",
        "timeout": 120000
    },
    "dismissable-top-fix": {
        "position": "top-fix",
        "dismissable": true,
        "icon": "it-error",
        "title": "Dismissable Top Fix",
        "text": "Dismissable Top Fix Content"
    },
    "dismissable-bottom-fix": {
        "position": "bottom-fix",
        "type": "error",
        "dismissable": true,
        "icon": "it-close-circle",
        "title": "Dismissable Bottom Fix",
        "text": "Dismissable Bottom Fix Content"
    },
    "dismissable-left-fix": {
        "position": "left-fix",
        "type": "info",
        "dismissable": true,
        "icon": "it-info-circle",
        "title": "Dismissable Left Fix",
        "text": "Dismissable Left Fix Content"
    },
    "dismissable-right-fix": {
        "position": "right-fix",
        "type": "warning",
        "dismissable": true,
        "icon": "it-error",
        "title": "Dismissable Right Fix",
        "text": "Dismissable Right Fix Content"
    }
}